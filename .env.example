# Database
LOCAL_DB=True
# Skip table init on server startup (set True after first init to avoid re-init)
TABLE_EXISTS=False

# Grid workflow
GRID_STREAM_ENABLED=false
GRID_CFG_PATH=test_out.json
GRID_CMD={python} -m jax_test.grid --cfg {cfg_path}
GRID_MODEL_OUT=model_out.json

# GCP (when LOCAL_DB=False)
# GCP_PROJECT_ID=your-project-id
# GCP_REGION=us-central1
# GEMINI_API_KEY=your-gemini-api-key

# Admin / gke_admin: deploy built images to Kubernetes
# Set LOCAL=true (or DEPLOY_LOCAL=true) to deploy to local cluster (current kubectl context, e.g. kind/minikube)
# Otherwise uses GKE cluster from:
# GKE_CLUSTER_NAME=your-gke-cluster
# GCP_PROJECT_ID=your-project-id
# GCP_REGION=us-central1
# GCP_ARTIFACT_REPO=qfs-repo
